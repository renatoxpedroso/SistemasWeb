                                                {
                                                    <div class="col-sm-5">
                                                        <input type="text" required="required" class="form-control" value="@usu.Nome" name="usuarios.nome" id="nome" placeholder="Nome Usuário">
                                                    </div>
                                                }
                                                else
                                                {
                                                    <div class="col-sm-5">
                                                        <input type="text" required="required" class="form-control" name="usuarios.nome" id="nome" placeholder="Nome Usuário">
                                                    </div>
                                                }
                                            }
                                        </div>

                                        <div class="form-group row">
                                            <label for="email" class="col-lg-2 col-form-label">E-mail*</label>
                                            @{
                                                if (usu != null)
                                                {
                                                    <div class="col-sm-5">
                                                        <input type="email" required="required" class="form-control" value="@usu.Email" name="usuarios.email" id="email" placeholder="E-mail">
                                                    </div>
                                                }
                                                else
                                                {
                                                    <div class="col-sm-5">
                                                        <input type="email" required="required" class="form-control" name="usuarios.email" id="email" placeholder="E-mail">
                                                    </div>
                                                }
                                            }
                                        </div>

                                        <div class="form-group row">
                                            <label for="senha" class="col-lg-2 col-form-label">Senha*</label>
                                            @{
                                                if (usu != null)
                                                {
                                                    <div class="col-sm-5">
                                                        <input type="password" required="required" class="form-control" value="@usu.Senha" name="usuarios.senha" id="senha" placeholder="Senha">
                                                    </div>
                                                }
                                                else
                                                {
                                                    <div class="col-sm-5">
                                                        <input type="password" required="required" class="form-control" name="usuarios.senha" id="senha" placeholder="Senha">
                                                    </div>
                                                }
                                            }
                                        </div>

                                        <div class="form-group row">
                                            <label for="datanacimento" class="col-lg-2 col-form-label">Data de Nascimento</label>
                                            @{
                                                if (usu != null)
                                                {
                                                    <div class="col-sm-3">
                                                        <input type="date" required="required" class="form-control" value="@usu.DataNacimento" name="usuarios.datanacimento" id="datanacimento" placeholder="Data de Nascimento">
                                                    </div>
                                                }
                                                else
                                                {
                                                    <div class="col-sm-3">
                                                        <input type="date" required="required" class="form-control" name="usuarios.datanacimento" id="datanacimento" placeholder="Data de Nascimento">
                                                    </div>
                                                }
                                            }
                                        </div>

                                        <div class="form-group row">
                                            <label for="cpfCnpj" class="col-lg-2 col-form-label">CPF*</label>
                                            @{
                                                if (usu != null)
                                                {
                                                    <div class="col-sm-3">
                                                        <input type="text" required="required" class="form-control" value="@usu.cpfCnpj" name="usuarios.cpfCnpj" id="cpfCnpj" placeholder="CPF">
