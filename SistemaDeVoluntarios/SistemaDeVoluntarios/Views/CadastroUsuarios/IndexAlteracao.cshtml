                                <!-- /.box-header -->
                                <!-- form start -->
                                <form class="form" method="post" action="/CadastroUsuarios/Alterar">
                                    <div style="margin-left:50px;" class="box-body">

                                        <div class="form-group">
                                            <div class="box-header with-border">
                                                <h3 class="box-title">Usuário</h3>
                                            </div>

                                            <label for="tipousuario">Tipo de Usuário</label>

                                            @{
                                                var usu = ViewBag.usuario;

                                                if (usu != null)
                                                {
                                                    if (usu.TipoUsuario == 1)
                                                    {
                                                        <div class="form-check form-check-inline">
                                                            <label class="form-check-label">
                                                                <input class="form-check-input" type="radio" name="usuarios.tipousuario" id="tipousuario" value="1" checked> Entidade
                                                            </label>
                                                        </div>

                                                        <div class="form-check form-check-inline">
                                                            <label class="form-check-label">
                                                                <input class="form-check-input" type="radio" name="usuarios.tipousuario" id="tipousuario" value="2"> Voluntário
                                                            </label>
                                                        </div>
                                                    }
                                                    else
                                                    {
                                                        <div class="form-check form-check-inline">
                                                            <label class="form-check-label">
                                                                <input class="form-check-input" type="radio" name="usuarios.tipousuario" id="tipousuario" value="1"> Entidade
                                                            </label>
                                                        </div>

                                                        <div class="form-check form-check-inline">
                                                            <label class="form-check-label">
                                                                <input class="form-check-input" type="radio" name="usuarios.tipousuario" id="tipousuario" value="2" checked> Voluntário
                                                            </label>
                                                        </div>
                                                    }

                                                }
                                                else
                                                {
                                                    <div class="form-check form-check-inline">
                                                        <label class="form-check-label">
                                                            <input class="form-check-input" type="radio" name="usuarios.tipousuario" id="tipousuario" value="1" checked> Entidade
                                                        </label>
                                                    </div>

                                                    <div class="form-check form-check-inline">
                                                        <label class="form-check-label">
                                                            <input class="form-check-input" type="radio" name="usuarios.tipousuario" id="tipousuario" value="2"> Voluntário
                                                        </label>
                                                    </div>
                                                }
                                            }
                                        </div>

                                        <div class="form-group">
                                            <div class="box-header with-border">
                                                <h3 class="box-title">Dados Pessoais</h3>
                                            </div>

                                            <label for="tipopessoa">Tipo de Pessoa</label>
                                            @{
                                                if (usu != null)
                                                {
                                                    if (usu.TipoPessoa == 1)
                                                    {
                                                        <div class="form-check form-check-inline">
                                                            <label class="form-check-label">
                                                                <input class="form-check-input" type="radio" name="usuarios.tipopessoa" id="tipopessoa" value="1" checked> Física
                                                            </label>
                                                        </div>

                                                        <div class="form-check form-check-inline">
                                                            <label class="form-check-label">
                                                                <input class="form-check-input" type="radio" name="usuarios.tipopessoa" id="tipopessoa" value="2"> Jurídica
                                                            </label>
                                                        </div>
                                                    }
                                                    else
                                                    {
                                                        <div class="form-check form-check-inline">
                                                            <label class="form-check-label">
                                                                <input class="form-check-input" type="radio" name="usuarios.tipopessoa" id="tipopessoa" value="1"> Física
                                                            </label>
                                                        </div>

                                                        <div class="form-check form-check-inline">
                                                            <label class="form-check-label">
                                                                <input class="form-check-input" type="radio" name="usuarios.tipopessoa" id="tipopessoa" value="2" checked> Jurídica
                                                            </label>
                                                        </div>
                                                    }
                                                }
                                                else
                                                {
                                                    <div class="form-check form-check-inline">
                                                        <label class="form-check-label">
                                                            <input class="form-check-input" type="radio" name="usuarios.tipopessoa" id="tipopessoa" value="1" checked> Física
                                                        </label>
                                                    </div>

                                                    <div class="form-check form-check-inline">
                                                        <label class="form-check-label">
                                                            <input class="form-check-input" type="radio" name="usuarios.tipopessoa" id="tipopessoa" value="2"> Jurídica
                                                        </label>
                                                    </div>
                                                }

                                            }


                                        </div>
                                        <div class="form-group row">
                                            <label for="nome" class="col-sm-2 col-form-label">Nome*</label>
