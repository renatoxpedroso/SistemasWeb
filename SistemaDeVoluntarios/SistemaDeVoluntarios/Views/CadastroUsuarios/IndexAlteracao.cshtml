                                                else
                                                {
                                                    <div class="col-sm-3">
                                                        <input type="text" required="required" class="form-control" name="usuarios.cpfCnpj" id="cpfCnpj" placeholder="CPF">
                                                    </div>
                                                }
                                            }
                                        </div>

                                        <div class="form-group row">
                                            <label for="telefone" class="col-lg-2 col-form-label">Telefone</label>
                                            @{
                                                if (usu != null)
                                                {
